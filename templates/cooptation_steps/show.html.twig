{% extends 'base.html.twig' %}

{% block title %}CooptationSteps{% endblock %}

{% block body %}
    <h1>CooptationSteps</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cooptation_step.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ cooptation_step.status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ cooptation_step.createdAt ? cooptation_step.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ cooptation_step.updatedAt ? cooptation_step.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cooptation_steps_index') }}">back to list</a>

    <a href="{{ path('app_cooptation_steps_edit', {'id': cooptation_step.id}) }}">edit</a>

    {{ include('cooptation_steps/_delete_form.html.twig') }}
{% endblock %}
