{% extends 'base.html.twig' %}

{% block title %}
	User
{% endblock %}

{% block body %}
	<div class="show-wrapper">
		<h1>
			Compétence :
			{{ skills.name }}
		</h1>

		<a class="devisio-btn devisio-btn-back-to-list" href="{{ path('back_app_skills_index') }}">Retour à la liste</a>

		<table>
			<tbody>
            <tr>
                <th>Id</th>
                <td>{{ skills.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ skills.name }}</td>
            </tr>
        </tbody>
		</table>

		<div class="show-buttons">
			{{ include('back/skills/_delete_form.html.twig') }}
			<a class="devisio-btn devisio-btn-update" href="{{ path( 'back_app_skills_edit', { id: skills.id } ) }}">Modifier</a>
		</div>
	</div>
{% endblock %}

