{% extends 'base.html.twig' %}

{% block title %}
	User
{% endblock %}

{% block body %}
	<div class="show-wrapper">
		<h1>
			Cooptation pour
            {{ cooptation.firstname  }}
            {{ cooptation.lastname }}


		</h1>

		<a class="devisio-btn devisio-btn-back-to-list" href="{{ path('back_app_cooptation_index') }}">Retour Ã  la liste</a>

		<table>
       <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cooptation.id }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ cooptation.firstname }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ cooptation.lastname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ cooptation.email }}</td>
            </tr>
            <tr>
                <th>CvPath</th>
                <td>{{ cooptation.cvName }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ cooptation.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ cooptation.createdAt ? cooptation.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ cooptation.updatedAt ? cooptation.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>

		</table>

		<div class="show-buttons">
			{{ include('back/cooptation/_delete_form.html.twig') }}
			<a class="devisio-btn devisio-btn-update" href="{{ path( 'back_app_cooptation_edit', { id: cooptation.id } ) }}">Modifier</a>
		</div>
	</div>
{% endblock %}


