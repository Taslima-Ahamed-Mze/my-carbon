{% extends 'base.html.twig' %}

{% block title %}Cooptation{% endblock %}

{% block body %}
    <h1>Cooptation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cooptation.id }}</td>
            </tr>
            <tr>
                <th>Firstname</th>
                <td>{{ cooptation.firstname }}</td>
            </tr>
            <tr>
                <th>Lastname</th>
                <td>{{ cooptation.lastname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ cooptation.email }}</td>
            </tr>
            <tr>
                <th>CvPath</th>
                <td>{{ cooptation.cvPath }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ cooptation.status }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ cooptation.createdAt ? cooptation.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ cooptation.updatedAt ? cooptation.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_app_cooptation_index') }}">back to list</a>

    <a href="{{ path('back_app_cooptation_edit', {'id': cooptation.id}) }}">edit</a>

    {{ include('back/cooptation/_delete_form.html.twig') }}
{% endblock %}
